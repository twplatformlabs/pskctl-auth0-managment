{
  "name": "dev-basic-authentication",
  "is_token_endpoint_ip_header_trusted": false,
  "description": "EMPC platform labs dev basic oidc auth0 application definition",
  "is_first_party": true,
  "oidc_conformant": true,
  "refresh_token": {
      "expiration_type": "expiring",
      "leeway": 0,
      "token_lifetime": 604800,
      "infinite_token_lifetime": false,
      "idle_token_lifetime": 172800,
      "infinite_idle_token_lifetime": false,
      "rotation_type": "non-rotating"
  },
  "sso_disabled": false,
  "cross_origin_auth": false,
  "jwt_configuration": {
      "alg": "RS256",
      "lifetime_in_seconds": 3600
  },
  "token_endpoint_auth_method": "client_secret_post",
  "app_type": "regular_web",
  "grant_types": [
      "implicit",
      "authorization_code",
      "refresh_token",
      "client_credentials"
  ],
  "callbacks": [
    "https://alertmanager.sbx-i01-aws-us-east-1.twplatformlabs.org/oauth2/callback"
  ],
  "web_origins": [
    "https://*.twplatformlabs.org"
  ],
  "custom_login_page_on": true
}
